{
    "compilerOptions": {
      "module": "ES2022",
      "target": "ES2020",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "types": ["node"],
      "outDir": "dist"
    },
    "include": ["./**/*.ts"],
    "exclude": ["node_modules"]
  }
  